  .submit
    text-align center
    width 38px
    background rgb(12, 85, 55)
    margin auto
    border-radius 5px
    padding 5px
    cursor pointer
    &:before
      font-family 'esentialIcons'
      content '\e911'
      font-size 28px
    
      
  
