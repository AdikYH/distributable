    background rgb(94, 94, 94)
  
  //width 1000px
  //width 1000px
  margin 0 auto
  font-family 'roboto-thin', 'roboto-regular','Helvetica Neue',Helvetica,Arial,sans-serif
  color rgb(56, 89, 149)//rgb(46, 46, 46)
  .editor-title
    font-family 'roboto-regular','Helvetica Neue',Helvetica,Arial,sans-serif
    font-size 18px
    color white
    width 100%
    padding 10px
    box-sizing border-box
    .box
      display inline-block
      width 100%//180px
      
      text-align center
      span
        font-family 'roboto-regular','Helvetica Neue',Helvetica,Arial,sans-serif
        font-size 10px//12px
        padding 5px 20px
        margin 0px 5px
        border 0px
        color white
        border-radius 15px
      .ttSp
        background rgba(144, 197, 63, 1)
      .mpSp
        color black
        background rgba(235, 240, 241, 1)//rgba(61, 12, 90, 1)
      .mrSp
        background rgba(0, 161, 134, 1)
    .mini
      width 180px
      padding 5px
      font-size 10px
  .editor-header
    display none
    font-family 'roboto-thin', 'roboto-regular','Helvetica Neue',Helvetica,Arial,sans-serif
    color white//rgb(58, 50, 113)//rgb(244, 150, 0)
    text-align center
    padding 10px
    
    h4
      font-family 'roboto-regular','Helvetica Neue',Helvetica,Arial,sans-serif
