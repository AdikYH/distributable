.modal
  display none /* Hidden by default */
  position fixed
  z-index 1
  padding-top 100px
  left 0
  top 0
  width 100%
  height 100%
  overflow auto
  background-color rgb(0,0,0)
  background-color rgba(0,0,0,0.4)
  .modal-content
    font-family 'roboto-light', Arial, sans-serif
    position relative
    background-color rgba(255, 255, 255, 0.5)//#fefefe
    margin auto
    padding 0
    //border 1px solid rgb(224, 54, 38)
    width 45%
    box-shadow 0 4px 8px 0 rgba(0,0,0,0.2),0 6px 20px 0 rgba(0,0,0,0.19)
    animation-name animatetop
    animation-duration 0.4s
    .modal-header
      padding 2px 10px
      font-size 14px
      background-color rgb(147, 32, 21)//rgb(224, 54, 38)//#5cb85c
      color white
      .close
        font-family Arial, sans-serif
        padding 2px
        margin 0px
        color white
        float right
        font-size 20px
        cursor pointer
        &:hover
          color black
    .modal-body
      background-color rgba(255, 255, 255, 0.6)
      padding 15px 20px 15px 20px
      span
        font-family 'roboto-black', Arial, sans-serif
    .modal-buttons
      background-color rgba(255, 255, 255, 0.6)
      padding 0px 40px 40px 40px
      .btn-Aceptar
        float left
        font-size 16px
        padding 5px
        border-radius 2px
        color white
        background rgb(73, 126, 98)//rgb(93, 156, 84)
        border 0px solid black
        cursor pointer
      .btn-Cancelar
        float right
        font-size 16px
        padding 5px
        border-radius 2px
        color white
        background rgb(230, 81, 61)//rgb(147, 32, 21)
        border 0px solid black
        cursor pointer
    .modal-footer
      padding 2px 10px
      background-color rgb(224, 54, 38)//#5cb85c
      color white

