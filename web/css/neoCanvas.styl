@keyframes growing {
  0% {left: -150px}
  100% {left: 5px}
}

@keyframes decrease {
  0% {left: 5px}
  100% {left: -150px}
}

@keyframes changeDocument {
  0% {color: rgba(38, 67, 111, 1)}
  100% {color: rgba(0, 24, 61, 1)}
}

@keyframes restoreDocument {
  0% {color: rgba(0, 24, 61, 1)}
  100% {color: rgba(38, 67, 111, 1)}
}

//rgba(0, 34, 88, 1)//rgba(12, 85, 55, 1)
//rgba(0, 128, 187, 1)//rgba(0, 161, 134, 1)

@keyframes changeColor {
  0% {background: rgba(0, 34, 88, 1)/*rgb(12, 85, 55)*/}
  100% {background: rgba(0, 128, 187, 1)/*rgb(0, 161, 134)*/}
}

@keyframes restoreColor {
  0% {background: rgba(0, 128, 187, 1)/*rgb(0, 161, 134)*/}
  100% {background: rgba(0, 34, 88, 1)/*rgb(12, 85, 55)*/}
}

@keyframes changeCreate {
  0% {color: rgba(83, 172, 68, 0.4)}
  100% {color: rgba(83, 172, 68, 1)}
}

@keyframes restoreCreate {
  0% {color: rgba(83, 172, 68, 1)}
  100% {color: rgba(83, 172, 68, 0.4)}
}

@keyframes changeDelete {
  0% {color: rgba(245, 66, 66, 0.4)}
  100% {color: rgba(245, 66, 66, 1)}
}

@keyframes restoreDelete {
  0% {color: rgba(245, 66, 66, 1)}
  100% {color: rgba(245, 66, 66, 0.4)}
}

@keyframes changeControls {
  0% {color: rgba(255, 255, 255, 0.4)}
  100% {color: rgba(255, 255, 255, 1)}
}

@keyframes restoreControls {
  0% {color: rgba(255, 255, 255, 1)}
  100% {color: rgba(255, 255, 255, 0.4)}
}

@keyframes changeInput {
  0% {color: rgba(255, 197, 0, 0.6); border-bottom: 2px solid rgba(255, 197, 0, 0.0);}
  100% {color: rgba(255, 197, 0, 1); border-bottom: 2px solid rgba(255, 197, 0, 1);}
}

@keyframes restoreInput {
  0% {color: rgba(255, 197, 0, 1); border-bottom: 2px solid rgba(255, 197, 0, 1);}
  100% {color: rgba(255, 197, 0, 0.6); border-bottom: 2px solid rgba(255, 197, 0, 0.0);}
}

@keyframes changeInputTag {
  0% {color: rgba(255, 255, 255, 0.6); border-bottom: 2px solid rgba(255, 255, 255, 0.0);}
  //0% {color: rgba(255, 197, 0, 0.6); border-bottom: 2px solid rgba(255, 197, 0, 0.0);}
  //100% {color: rgba(255, 197, 0, 1); border-bottom: 2px solid rgba(255, 197, 0, 1);}
  100% {color: rgba(255, 255, 255, 1); border-bottom: 2px solid rgba(255, 255, 255, 1);}
}

@keyframes restoreInputTag {
  0% {color: rgba(255, 255, 255, 1); border-bottom: 2px solid rgba(255, 255, 255, 1);}
  100% {color: rgba(255, 255, 255, 0.6); border-bottom: 2px solid rgba(255, 255, 255, 0.0);}
}

@keyframes animatetop {
  from {top:-300px; opacity:0}
  to {top:0; opacity:1}
}

@keyframes animateDraw {
  from {display: none}
  to {display: block}
}
@keyframes animateRDraw {
  from {display: block}
  to {display: none}
