.dash-title
  padding 25px 0px 25px 0px
  margin 0 auto
  .element-title
    text-align center
    font-family 'roboto-light', 'roboto-regular', 'Helvetica Neue', Helvetica, Arial, sans-serif, 'roboto-condensed'
    color white//rgb(7, 102, 146)//rgb(3, 19, 53)//white//#2E2E2E//black//#0080BB//#60BC03
    font-size 24px
.dash-datetime
  //margin 20px auto
  font-family 'roboto-light', 'roboto-regular', 'Helvetica Neue', Helvetica, Arial, sans-serif, 'roboto-condensed'
  color white
  max-width 300px
  margin 20px auto
  background rgba(17, 116, 76, 1)
  //box-sizing border-box
  border-radius 20px
  transition 1s
  &:hover
    background rgba(17, 116, 76, 0.8)
  .dash-icontime
    display inline-block
    width 50%
    box-sizing border-box
    padding 40px 28px 30px 28px
    font-size 100px
    //font-size 120px
    &:after
      font-family 'dashIcons'
      content '\e94e'
  .dash-rdata
    border 1px solid white
    border-right 0px
    border-top 0px
    border-bottom 0px
    box-sizing border-box
    padding 20px 10px
    display inline-block
    width 50%
    .dash-data
      font-size 14px
    .dash-time
      font-size 48px
      position relative
      span
        position absolute
        top 15px
        //padding-left 2px
        //margin-bottom 100px
        font-size 10px
      
