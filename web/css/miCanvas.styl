@font-face {
	font-family: 'grades';
	src:url('fonts/icomoon.eot?t05rsc');
	src:url('fonts/icomoon.eot?t05rsc#iefix') format('embedded-opentype'),
		url('fonts/icomoon.ttf?t05rsc') format('truetype'),
		url('fonts/icomoon.woff?t05rsc') format('woff'),
		url('fonts/icomoon.svg?t05rsc#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}
