              border-width 0px 2px 2px 2px
              border-style solid
              border-color black
              padding-bottom 2px
              margin-bottom 5px
            &:first-child
              border-width 2px 2px 0px 2px
              border-style solid
              border-color black
              padding-top 2px
              margin-top 5px
.menu-angle
  width 170px
  display table
  table-layout fixed
  .cabecera
    margin 5px 0px 0px 0px
    text-align center
    font-size 14px
    color white
    padding 10px
