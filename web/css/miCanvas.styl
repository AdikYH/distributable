@import 'nib/vendor'
vendor-prefixes = ms o moz webkit

body
  margin 0px
  padding 0px
  font-family 'roboto', 'Helvetica Neue', Helvetica, Arial, sans-serif
  //background url('../images/backgroundbn.jpg') no-repeat fixed center rgba(61, 61, 61,0.8)//rgb(241, 83, 0)
  background rgba(23, 23, 23, 0.9)//rgba(15, 10, 51, 0.9)//rgb(58, 50, 113)//rgb(72, 61, 139)//rgba(52, 20, 39, 0.8)
  background-size cover
.menu
  position absolute
  top 50px
  width 20%
  height 400px
  float left
  margin 5px
  
.datos-proyecto
  width 170px
  .cabecera
    text-align center
    font-size 14px
    color white
    padding 10px
    background #365D2C
    border 2px solid black
  .cuerpo
    ul
      padding 0px
      margin 0px
      li
        text-transform none !important
        border-width 2px
        border-style solid
        border-color black
        padding 0px 2px
        width 146px
        display block
        position relative
        text-align center
        font-size 14px
        color white
        padding 10px
        background #365D2C
        //color white
        &:hover > ul
          display block
        ul
          top 43px
          left 0
          display none
          position absolute
          padding 0px
          margin 0px
          li
            text-align left
            border-width 0px 2px 0px 2px
            border-style solid
            border-color black
            padding 0px
            margin 0px
            background #636363
            width 162px
            display block
            font-size 10px
