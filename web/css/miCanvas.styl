.menu-component
  display none
  margin-top 5px
  margin-bottom 5px
  width 170px
  .cabecera
    text-align center
    font-size 14px
    color white
    padding 10px
    background #FF9D00
    border 2px solid black
  select
    width 170px
    background #eee url('../images/arrow.png')
    background-position 230px center
    background-repeat no-repeat
    padding 5px//10px
    font-size 14px//16px
    border 0
    border-radius 3px
    appearance none
    
    transition all 0.4s
    
    box-shadow 0 1px 2px rgba(0,0,0,0.3)
  
  select:hover
    background-color #ddd
    width 260px
    
  input[type = 'checkbox']
    user-select none
    
.menu-distance
  width 170px
  padding 2%
  text-align center
  .distance
    width 70%
    font-size 21px
    border 2px solid #929292
    padding 10px
    margin 5px
    border-radius 5px
    &:focus
      outline none
      border 2px solid #4A7B3E
      box-shadow 0px 0px 20px #4A7B3E
      
.menu-button
  width 170px
  text-align center
  .button
    background #365D2C
    border-radius 5px
    padding 10px 60px
    margin 2px 0px
    font-size 14px
    color white
    text-decoration none
    border 2px solid black
    &:hover
      background #378228
      text-decoration none
    &:focus
      outline none
  form
    input[type = 'text']
      display none
    input[type = 'submit']
      background rgba(147, 32, 21, 0.8)
      border-radius 5px
      padding 10px 56.5px
      margin 2px 0px
      font-size 14px
      color white
      text-decoration none
      border 2px solid black
      &:hover
        background rgba(147, 32, 21, 1)
        text-decoration none
      &:focus
        outline none
  .button-cleaner
    background rgba(255, 157, 0, 0.8)
    border-radius 5px
    padding 10px 59.5px
    margin 2px 0px
    font-size 14px
    color white
    text-decoration none
    border 2px solid black
    &:hover
      background rgba(255, 157, 0, 1)
      text-decoration none
    &:focus
      outline none
      
  .button-breaker
    background rgba(0, 161, 134, 0.8)//rgba(255, 157, 0, 0.8)
    border-radius 5px
    padding 10px 57px
    margin 2px 0px
    font-size 14px
    color white
    text-decoration none
    border 2px solid black
    &:hover
      background rgba(0, 161, 134, 1)//rgba(255, 157, 0, 1)
      text-decoration none
    &:focus
      outline none
  .button-adder
    background rgba(0, 128, 187, 0.8)//rgba(0, 161, 134, 0.8)
    border-radius 5px
    padding 10px 48.5px
    margin 2px 0px
    font-size 14px
    color white
    text-decoration none
    border 2px solid black
    &:hover
      background rgba(0, 128, 187, 1)//rgba(0, 161, 134, 1)
      text-decoration none
    &:focus
      outline none
      
      
.contenedor
  
  canvas(id = 'boceto' class ='canvas' width = 681 height =718) //684 721
  margin 0 auto
  padding 10px 0px 0px 0px//100px 0px
  width 681px
  display block
  canvas
    background rgba(255, 255, 255, 0.8)//rgba(255, 255, 255, 0.3)
    //background url('../images/pap-pattern-2.jpg') repeat
    border 0px solid rgba(61, 61, 61,0.8)
    //float left
.tablas
  //filter opacity(0.6)
  
  position absolute
  top 50px
  right 0px
  color black  
  table
    background white
    border-collapse collapse
    
    width 280px
    tr
      border 0px solid white
      transition all 0.4s
      .center
        text-align center
      /*.strech        
        display none
        display inline-block*/
      th
        font-family 'roboto-light', 'Helvetica Neue', Helvetica, Arial, sans-serif
        //margin 1px solid white
        padding 10px
        background rgba(58, 50, 113, 1)//rgba(147, 32, 21, 1)
        color white
      td
        font-family 'roboto-light', 'Helvetica Neue', Helvetica, Arial, sans-serif
        font-size 12px
        padding 5px
        border 0px solid white
        margin 0
      &:nth-child(even)
        background rgba(58, 50, 113, 0.2)//rgba(147, 32, 21, 0.2)
      &:nth-child(odd)
        background rgba(58, 50, 113, 0.1)//rgba(147, 32, 21, 0.1)
    /*&:hover
      tr
        .strech
          display block*/
  .output
    background-color transparent

.clicked
  background-color #0C5537 !important
  &:before
    color white !important
    
    
.myFooterTable
  width 100%
  text-align center
  table
    border-collapse collapse
    tr
      th
        font-family 'Helvetica Neue', Helvetica, Arial, sans-serif
        color #DBDBDB
      td
        color #DBDBDB
  
  .symbolTable
    border-top 1px solid #DBDBDB
    border-bottom 1px solid #DBDBDB
    border-right 1px solid #DBDBDB
    float left
    width 50%
    height 200px
    font-size 10px
  .ft
    font-size 14px
  .mt
    font-size 10px
  .tlf
    text-align left
    padding-left 5px
    /*padding 2px 4px
    padding-left 5px
    padding-right 5px*/
  .ht
    font-style italic
    font-size 20px
  .middleTable
    border-top 1px solid #DBDBDB
    border-bottom 1px solid #DBDBDB
    float left
    width 20%
    height 200px
    font-size 10px
  
  .rightTable
    border-top 1px solid #DBDBDB
    border-bottom 1px solid #DBDBDB
    border-left 1px solid #DBDBDB
    width 30%
    height 200px
    font-size 10px
  .bt
    border-bottom 1px solid #DBDBDB
  .tp
    border-top 1px solid #DBDBDB
  .lf
    border-left 1px solid #DBDBDB
  .rg
    border-right 1px solid #DBDBDB
    
footer
  position relative
  p 
    display block
  .documentProject
    width 100%
    form
      .line-form
        margin 5px
        display block
        p
          font-size 18px
          color rgb(144, 197, 63)
        .line-part
