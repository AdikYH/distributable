.contenedor

  canvas(id = 'boceto' class ='canvas' width = 681 height =718)
      //684 721
  margin 0 auto
  padding-top 50px
  width 681px
  display block
  canvas
    background rgba(255, 255, 255, 0.3)
    //background url('../images/pap-pattern-2.jpg') repeat
    border 0px solid rgba(61, 61, 61,0.8)
    //float left
.tablas
  //filter opacity(0.6)
  position absolute
  top 50px
  right 0px
  color black
  table
    background white
    border-collapse collapse

    tr
      border 0px solid white
      transition all 0.4s
      .center
        text-align center
      .strech
        display none
      th
        font-family 'roboto-light', 'Helvetica Neue', Helvetica, Arial, sans-serif
        //margin 1px solid white
        padding 10px
        background rgba(58, 50, 113, 1)//rgba(147, 32, 21, 1)
        color white
      td
        font-family 'roboto-light', 'Helvetica Neue', Helvetica, Arial, sans-serif
        font-size 12px
        padding 5px
        border 0px solid white
        margin 0
      &:nth-child(even)
        background rgba(58, 50, 113, 0.2)//rgba(147, 32, 21, 0.2)
      &:nth-child(odd)
        background rgba(58, 50, 113, 0.1)//rgba(147, 32, 21, 0.1)
    &:hover
      tr
        .strech
          display block
  .output
    background-color transparent


.clicked
