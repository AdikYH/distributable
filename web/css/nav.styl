@import 'nib/vendor'
vendor-prefixes = ms o moz webkit

*
  padding 0
  margin 0

.lista
  color red
nav
  font-size 13px
  width 100%
  height 50px
  background-color rgb(56, 89, 149)//#353535//rgb(99, 99, 99)
  .menu-lit
    display none
    float left
    &:hover
      cursor pointer
    &:after
      font-family 'menuResponsive'//'navIcons'
      content '\e900'//'\e9bd'//''
      color white
      font-size 35px
      padding 5px 7px
      display inline-block
  .menu-nav
    //background  green
    //width 640px
    z-index 20
    margin 0 auto
    ul
      list-style none
      li
        float left
        text-transform uppercase
        position relative
        padding 4px
        width 140px
        height 24px
        margin-top 10px
        text-align center
        &:hover > ul
          display block
        a
          display block
          font-family "roboto-regular","Helvetica Neue",Helvetica,Arial,sans-serif
          color white
          text-decoration none
          &:hover
            color #FCD85F//rgb(250, 198, 19)//#60BC03//
        ul
          
          display none
          position absolute
          z-index 5
          top 30px
          li
            background-color rgb(56, 89, 149)//#353535//rgb(99, 99, 99)
            margin 0
            
            padding 5px
            a
              font-family "roboto-light","Helvetica Neue",Helvetica,Arial,sans-serif
            &:last-child
              border-radius 0px 0px 5px 5px//10px 10px
  .menu-lat
    float right
    ul
      list-style none
      li
        float left
        text-transform uppercase
        position relative
        height 30px
        margin-top 10px
        text-align center
        &:hover > ul
          display block
        a
          width 100px
          display block
          font-family "roboto-regular","Helvetica Neue",Helvetica,Arial,sans-serif
          color white//#ADD472//white
          text-decoration none
          &:hover
            color white//#ADD472//rgb(250, 198, 19)
          &:after
            font-family 'navIcons'
            font-size 20px//40px
            content '\e901'//\f049'
            color white//#90C53F//#427236
            padding 2px 5px
            display inline-block
        form
          input[type = 'text']
            font-family "roboto-regular","Helvetica Neue",Helvetica,Arial,sans-serif
            padding 4px
            margin 0px 2px
            width 150px
            border-radius 5px 0px 0px 5px
            border 0px
            float left
            color rgb(0, 128, 187)
          button
            margin 0px
            padding 0px
            border 0px
            float left
            background transparent//Swhite
            border-radius 0px 5px 5px 0px
            &:hover
              //background #FCD85F//#FAC613
              cursor pointer
            span
              &:before
                font-family 'navIcons'
                content '\f02e'
                font-size 18px
                color rgba(255, 255, 255, 0.8)//white//#616161//white//black
                padding 4px
                display inline-block
        ul
          display none
          li
            background-color rgb(56, 89, 149)//#353535//rgb(99, 99, 99)
            z-index 5
            margin 0px
            top 5px
            a
              display block
              font-family "roboto-regular","Helvetica Neue",Helvetica,Arial,sans-serif
              &:after
                font-family 'menuResponsive'//'navIcons'
                content '\ea14'//''
                color white
                padding 2px
                display none//inline-block
              &:hover
                color #FCD85F//white//#ADD472
            &:last-child
              border-radius 0px 0px 5px 5px//10px 10px

footer
  font-family 'roboto-light', 'roboto-regular', 'Helvetica Neue', Helvetica, Arial, sans-serif
  float left
  width 100%
  background rgb(62, 74, 123)//rgb(0, 152, 36)//#341427//black//#616161//#60BC03//
  text-align center
  padding 20px 0px
  color white
  margin-top 15px
  
  position fixed
  margin-bottom 0px
  bottom 0px
  border 0px
  
              
