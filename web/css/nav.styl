@media screen and (max-width 745px)//672px)
  nav
    .menu-lit
      display inline-block
    .menu-lat
      width calc(100% - 50px)
      ul
        li
          width calc(100% - 50px)
          a
            width 40px
            span
              display none
          form
            input[type = 'text']
              //float right
              width calc(100% - 52px)
          
          &:last-child
            width 40px
          ul
            li
              a
                &:after
                  display inline-block
    .menu-nav
      background rgb(62, 74, 123)
      color white
      //
      display none
      //
      position absolute
      top 50px
      width 80%
      ul
        //background rgb(62, 74, 123)
        width 100%
        li
          background rgb(62, 74, 123)
          width 100%
          position relative
          margin 0px
          span
            position absolute
            left 88%//5px
            top 2px
            //background rgba(20, 39, 52, 0.5)
            border-radius 2px
            &:before
              font-family 'menuResponsive'
              content '\e901'//'\e908'
              font-size 20px
            &:hover
              cursor pointer
              color rgb(144, 197, 63)//red
              //background rgb(20, 39, 52)
              //border-radius 2px
              //padding-top 2px
          .replace
            &:before
              font-family 'menuResponsive'
              content '\e90c'//'\e908'
              font-size 20px
          &:hover > ul
            display none
          /*a
            display block*/
          ul
            //background rgba(255, 255, 255, 0.5)
            margin 0px
            width 100%
            box-sizing border-box
            left 0px
            li
              padding-left 0px
              padding-right 0px
              a
                //width 100%
                //background rgba(255, 255, 255, 0.5)
                box-sizing border-box
                padding 5px 0px
